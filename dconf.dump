[org/gnome/GWeather]
temperature-unit='centigrade'

[org/gnome/desktop/input-sources]
sources=[('xkb', 'us+colemak'), ('xkb', 'us')]
xkb-options=['lv3:ralt_switch', 'ctrl:swapcaps']

[org/gnome/desktop/interface]
clock-show-seconds=true
clock-show-weekday=true
gtk-enable-primary-paste=true
gtk-im-module='gtk-im-context-simple'
gtk-theme='Arc'
icon-theme='Arc'
monospace-font-name='Fira Code 10'
show-battery-percentage=true

[org/gnome/desktop/peripherals/keyboard]
delay=uint32 250
repeat-interval=uint32 40

[org/gnome/desktop/peripherals/mouse]
accel-profile='flat'
speed=0.0

[org/gnome/desktop/session]
idle-delay=uint32 0

[org/gnome/desktop/wm/keybindings]
activate-window-menu=@as []
close=['<Super>w']
minimize=['<Super>z']
switch-applications=@as []
switch-applications-backward=@as []
switch-group=['<Super>grave', '<Alt>grave']
switch-group-backward=['<Shift><Super>grave', '<Shift><Alt>grave']
switch-windows=['<Super>Tab', '<Alt>Tab']
switch-windows-backward=['<Shift><Super>Tab', '<Shift><Alt>Tab']
toggle-fullscreen=['<Super>f']

[org/gnome/desktop/wm/preferences]
button-layout='appmenu:minimize,maximize,close'
resize-with-right-button=true

[org/gnome/mutter/keybindings]
switch-monitor=['<Super><Shift>p', 'XF86Display']

[org/gnome/settings-daemon/plugins/color]
night-light-enabled=true

[org/gnome/settings-daemon/plugins/media-keys]
custom-keybindings=['/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom2/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom3/']
screensaver=['<Super>i']
volume-down=['<Shift><Super>underscore']
volume-mute=['<Shift><Super>m']
volume-up=['<Shift><Super>plus']

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0]
binding='<Super>c'
command='/home/isabelle/.scripts/clearclipboard'
name='Clear Clipboard'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1]
binding='<Super>p'
command='/home/isabelle/.scripts/passmenu.sh'
name='Passmenu for password'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom2]
binding='<Super>u'
command='/home/isabelle/.scripts/passmenu.sh -u'
name='Passmenu for username'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom3]
binding=''
command='/usr/bin/kitty'
name='Kitty'

[org/gnome/terminal/legacy/profiles:/:b1dcc9dd-5262-4d8d-a863-c897e6d979b9]
background-color='rgb(237,237,236)'
bold-is-bright=true
palette=['rgb(0,0,0)', 'rgb(204,0,0)', 'rgb(78,154,5)', 'rgb(196,160,0)', 'rgb(52,100,164)', 'rgb(116,80,122)', 'rgb(5,152,154)', 'rgb(211,215,207)', 'rgb(84,87,83)', 'rgb(239,40,40)', 'rgb(138,226,52)', 'rgb(252,233,78)', 'rgb(113,158,207)', 'rgb(173,126,167)', 'rgb(52,226,226)', 'rgb(237,237,236)']
use-system-font=false
use-theme-colors=false
